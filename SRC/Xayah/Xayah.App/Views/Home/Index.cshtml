@{
	ViewData["Title"] = "Home Page";
}

<form method="post" enctype="multipart/form-data">	
	<div class="panel panel-primary">
		<div class="panel-heading">ConciliandoTransações</div>
		<div class="panel-body">
			<div class="col-md-12">
				<label for="inputFiles">Selecione os arquivos para conciliação:</label>
				<input type="file" name="files" multiple id="inputFiles" />
			</div>
		</div>
		<div class="col-lg-12 panel-footer">
			<input type="submit" value="Upload" class="btn btn-primary right" />
		</div>
	</div>
</form>
